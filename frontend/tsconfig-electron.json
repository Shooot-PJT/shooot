{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "ES6",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "public_electron",
    "baseUrl": ".",
    "paths": {
      "*": [
        "node_modules/*",
        "src_electron/*",
        "src_electron/renderer-process/*"
      ]
    }
  },
  "include": ["src_electron/**/*"]
}
